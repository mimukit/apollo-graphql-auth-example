{
  "extends": "airbnb-base",
  "rules": {
    "no-unused-vars": ["error", { "args": "none" }],
    "arrow-parens": 0,
    "function-paren-newline": 0,
    "implicit-arrow-linebreak": 0,
    "class-methods-use-this": 0,
    "no-param-reassign": 0,
    "no-underscore-dangle": 0,
    "no-confusing-arrow": 0,
    "operator-linebreak": 0
  }
}
